#
# My bashrc.
#

# gcc settings
export CC=gcc-4.5
export CXX=g++-4.5

# java settings
export JAVA_HOME=~/jdk1.6.0_33
export PATH=$PATH:$JAVA_HOME/bin

# android sdk/ndk setting
export ANDROID_JAVA_HOME=$JAVA_HOME
export PATH=$PATH:~/bin
ANDROID_SDK_HOME=~/android-sdks
export PATH=$PATH:$ANDROID_SDK_HOME/tools
export PATH=$PATH:$ANDROID_SDK_HOME/platform-tools
export PATH=$PATH:~/android-ndk-r8
export USE_CCACHE=1

# misc path settings
export PATH=$PATH:~/eclipse
export PATH=$PATH:~/cov-sa-linux64-5.5.3/bin

alias eclipse='eclipse >& /dev/null &'

